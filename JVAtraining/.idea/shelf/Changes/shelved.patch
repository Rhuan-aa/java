+package persistence;
+
+import controller.DtoMapper;
+import model.Player;
+import persistence.dao.PlayerDao;
+import persistence.dao.TeamDao;
+import persistence.team.PersistedTeamDto;
+import persistence.team.RegisterTeamDto;
+
+import java.sql.ResultSet;
+import java.sql.SQLException;
+import java.util.ArrayList;
+import java.util.List;
+import java.util.Optional;
+
+public class SQLiteTeamDao implements TeamDao {
+    @Override
+    public void register(RegisterTeamDto dto) {
+        String sql = "INSERT INTO teams (name, base_location, coach_name, id_captain) VALUES (?, ?, ?, ?)";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            statement.setString(1, dto.name());
+            statement.setString(2, dto.baseLocation());
+            statement.setString(3, dto.coachName());
+            statement.setInt(4, dto.id_captain());
+
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+
+    @Override
+    public void update(PersistedTeamDto dto) {
+        String sql = """
+                UPDATE teams SET 
+                    name = ?, 
+                    base_location = ?, 
+                    coach_name = ?, 
+                    id_captain = ?
+                WHERE
+                    id = ?""";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            statement.setString(1, dto.name());
+            statement.setString(2, dto.baseLocation());
+            statement.setString(3, dto.coachName());
+            statement.setInt(4, dto.captain().getId());
+            statement.setInt(5, dto.id());
+
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+
+    @Override
+    public Optional<PersistedTeamDto> findOne(Integer id) {
+        String sql = "SELECT * FROM teams WHERE id = ?";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            statement.setInt(1, id);
+            final ResultSet rs = statement.executeQuery();
+            PlayerDao dao = new SQLitePlayerDao();
+
+            if (rs.next()) {
+                List<Player> players = getPlayers(dao, id);
+
+                return Optional.of(new PersistedTeamDto(
+                        rs.getInt("id"),
+                        rs.getString("name"),
+                        rs.getString("base_location"),
+                        rs.getString("coach_name"),
+                        dao.findOne(rs.getInt("id_captain"))
+                                .map(DtoMapper::fromDto)
+                                .orElse(null),
+                        players
+                ));
+            }
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+
+        return Optional.empty();
+    }
+
+    @Override
+    public List<PersistedTeamDto> findAll() {
+        String sql = "SELECT * FROM teams";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            final ResultSet rs = statement.executeQuery();
+            List<PersistedTeamDto> teams = new ArrayList<>();
+            PlayerDao dao = new SQLitePlayerDao();
+
+            while (rs.next()) {
+                List<Player> players = getPlayers(dao, rs.getInt("id"));
+                PersistedTeamDto team = new PersistedTeamDto(
+                        rs.getInt("id"),
+                        rs.getString("name"),
+                        rs.getString("base_location"),
+                        rs.getString("coach_name"),
+                        dao.findOne(rs.getInt("id_captain"))
+                                .map(DtoMapper::fromDto)
+                                .orElse(null),
+                        players
+                );
+                teams.add(team);
+            }
+            return teams;
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+
+        return List.of();
+    }
+
+    private static List<Player> getPlayers(PlayerDao dao, Integer id) {
+        return dao.findAll().stream()
+                .filter(p -> p.teamId() == id)
+                .map(DtoMapper::fromDto)
+                .toList();
+    }
+
+    @Override
+    public void delete(Integer id) {
+        String sql = "DELETE FROM teams WHERE id = ?";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            statement.setInt(1, id);
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+}
