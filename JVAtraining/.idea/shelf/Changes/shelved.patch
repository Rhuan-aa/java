+        System.out.println("Times cadastrados:");
+        for (PersistedTeamDto t : teamDao.findAll()) {
+            System.out.println(t.name());
+        }
+
+        // 4. Deletar time
+        if (teamPersisted.isPresent()) {
+            teamDao.delete(teamPersisted.get().id());
+            System.out.println("Time deletado.");
+        }
+
+        // 5. Testar findOne do jogador
+        Optional<PersistedPlayerDto> playerFound = playerDao.findOne(messiPersisted.get().id());
+        if (playerFound.isPresent()) {
+            System.out.println("Jogador encontrado: " + playerFound.get().name());
+        } else {
+            System.out.println("Jogador não encontrado.");
+        }
+    }
