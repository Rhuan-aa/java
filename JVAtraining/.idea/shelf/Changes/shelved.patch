+package controller;
+
+import model.Player;
+import model.Team;
+import persistence.SQLitePlayerDao;
+import persistence.SQLiteTeamDao;
+import persistence.dao.PlayerDao;
+import persistence.dao.TeamDao;
+import persistence.player.PersistedPlayerDto;
+import persistence.player.RegisterPlayerDto;
+import persistence.team.PersistedTeamDto;
+import persistence.team.RegisterTeamDto;
+
+import java.util.List;
+
+public class DtoMapper {
+
+    private DtoMapper() {}
+
+    public static RegisterPlayerDto toDto(Player player) {
+        return new RegisterPlayerDto(
+                player.getName(),
+                player.getNumber(),
+                player.getPosition(),
+                player.isFielded(),
+                player.getTeam().getId()
+        );
+    }
+
+    public static RegisterTeamDto toDto(Team team) {
+        return new RegisterTeamDto(
+                team.getName(),
+                team.getBaseLocation(),
+                team.getCoachName(),
+                team.getCaptain().getId()
+        );
+    }
+
+    public static Player fromDto(PersistedPlayerDto dto) {
+        return new Player(
+                dto.id(),
+                dto.name(),
+                dto.number(),
+                dto.position(),
+                dto.isFielded()
+        );
+    }
+
+    public static Team fromDto(PersistedTeamDto dto) {
+        Team team = new Team(
+                dto.id(),
+                dto.name(),
+                dto.baseLocation(),
+                dto.coachName(),
+                dto.captain()
+        );
+
+        if (!dto.players().isEmpty()) {
+            for (Player player : dto.players())
+                team.addPlayer(player);
+        }
+
+        return team;
+    }
+}
