+package br.ifsp.infection.services;
+
+import br.ifsp.infection.model.Bulletin;
+import br.ifsp.infection.model.State;
+
+import java.time.LocalDate;
+import java.util.List;
+
+public class FilterBulletinService {
+
+    public FilterBulletinService() {
+    }
+
+    public List<Bulletin> filter(
+            List<Bulletin> allBulletin,
+            String city,
+            State state,
+            LocalDate begin,
+            LocalDate end
+    ) {
+        return allBulletin.stream()
+                .filter(bulletin -> filterByCity(city, bulletin))
+                .filter(bulletin -> filterByState(state, bulletin))
+                .filter(bulletin -> isBeforeBegin(begin, bulletin))
+                .filter(bulletin -> isAfterEnd(end, bulletin))
+                .toList();
+    }
+
+    private static boolean isAfterEnd(LocalDate end, Bulletin bulletin) {
+        return end == null || !bulletin.getDate().isAfter(end);
+    }
+
+    private static boolean isBeforeBegin(LocalDate begin, Bulletin bulletin) {
+        return begin == null || !bulletin.getDate().isBefore(begin);
+    }
+
+    private static boolean filterByState(State state, Bulletin bulletin) {
+        return state == null || bulletin.getState() == state;
+    }
+
+    private static boolean filterByCity(String city, Bulletin bulletin) {
+        return city == null || bulletin.getCity().toLowerCase().contains(city.toLowerCase());
+    }
+}
