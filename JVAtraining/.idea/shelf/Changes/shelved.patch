+package persistence;
+
+import exception.EntityAlreadyExistsException;
+import model.InstitutionalMember;
+import model.Professor;
+
+import java.util.*;
+
+public class InMemoryMemberRepository implements InstitutionalMemberRepository {
+    private final Map<String, InstitutionalMember> db = new LinkedHashMap<>();
+
+    @Override
+    public void save(InstitutionalMember member) {
+        if (db.putIfAbsent(member.getCpf(), member) != null)
+            throw new EntityAlreadyExistsException("Member is already registered");
+    }
+
+    @Override
+    public void update(InstitutionalMember member) {
+        if (db.replace(member.getCpf(), member) == null)
+            throw new NoSuchElementException("Member isn't registered");
+    }
+
+    @Override
+    public Optional<InstitutionalMember> findById(String cpf) {
+        return Optional.ofNullable(db.get(cpf));
+    }
+}
