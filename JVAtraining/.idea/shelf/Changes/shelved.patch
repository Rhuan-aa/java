+package model;
+
+import exception.EntityAlreadyExistsException;
+
+import java.time.LocalDate;
+import java.util.HashSet;
+import java.util.Objects;
+import java.util.Set;
+
+public sealed abstract class InstitutionalMember permits Researcher, Professor {
+    protected final String cpf;
+    protected String name;
+    protected LocalDate entryDate;
+    protected Professor advisor;
+    protected final Set<Publication> publications;
+
+    public InstitutionalMember(String cpf, String name, LocalDate entryDate, Professor advisor) {
+        this.cpf = cpf;
+        this.name = name;
+        this.entryDate = entryDate;
+        this.advisor = advisor;
+        this.publications = new HashSet<>();
+    }
+
+    public abstract double calculateScore();
+
+    @Override
+    public boolean equals(Object o) {
+        if (o == null || getClass() != o.getClass()) return false;
+        InstitutionalMember that = (InstitutionalMember) o;
+        return Objects.equals(cpf, that.cpf);
+    }
+
+    @Override
+    public int hashCode() {
+        return Objects.hashCode(cpf);
+    }
+
+    @Override
+    public String toString() {
+        return String.format("[%s] %s | Ingresso: %s", cpf, name, entryDate);
+    }
+
+    public void addPublication(Publication publication) {
+        if (publications.contains(publication))
+            throw new EntityAlreadyExistsException("Publication is already published");
+        publications.add(publication);
+    }
+
+    public String getCpf() {
+        return cpf;
+    }
+
+    public String getName() {
+        return name;
+    }
+
+    public void setName(String name) {
+        this.name = name;
+    }
+
+    public LocalDate getEntryDate() {
+        return entryDate;
+    }
+
+    public void setEntryDate(LocalDate entryDate) {
+        this.entryDate = entryDate;
+    }
+
+    public Professor getAdvisor() {
+        return advisor;
+    }
+
+    public void setAdvisor(Professor advisor) {
+        this.advisor = advisor;
+    }
+
+    public Set<Publication> getPublications() {
+        return new HashSet<>(publications);
+    }
+}
