+package br.ifsp.persistence;
+
+import br.ifsp.services.EmployeeDTO;
+
+import java.sql.ResultSet;
+import java.sql.SQLException;
+import java.time.LocalDate;
+import java.util.ArrayList;
+import java.util.List;
+import java.util.Optional;
+
+public class SQLiteEmployeeDAO implements EmployeeDAO{
+    @Override
+    public void save(EmployeeDTO employee) {
+        String sql = """
+                INSERT INTO employee (
+                    id, 
+                    name, 
+                    birth_date, 
+                    sold_value, 
+                    consultant_in_charge
+                ) VALUES (?, ?, ?, ?, ?)""";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            statement.setString(1, employee.id());
+            statement.setString(2, employee.name());
+            statement.setString(3, String.valueOf(employee.birthDate()));
+            statement.setDouble(4, employee.soldValue());
+            statement.setString(5, employee.inChargeId());
+
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+
+    @Override
+    public void update(EmployeeDTO employee) {
+        String sql = """
+                UPDATE employee SET 
+                    name = ?, 
+                    birth_date = ?, 
+                    sold_value = ?, 
+                    consultant_in_charge = ?
+                WHERE
+                    id = ?    
+                """;
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            statement.setString(1, employee.name());
+            statement.setString(2, String.valueOf(employee.birthDate()));
+            statement.setDouble(3, employee.soldValue());
+            statement.setString(4, employee.inChargeId());
+            statement.setString(5, employee.id());
+
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+
+    @Override
+    public Optional<EmployeeDTO> findById(String id) {
+        String sql = "SELECT * FROM employee WHERE id = ?";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            statement.setString(1, id);
+            final ResultSet rs = statement.executeQuery();
+
+            if (rs.next()) {
+                return Optional.of(new EmployeeDTO(
+                        rs.getString("id"),
+                        rs.getString("name"),
+                        LocalDate.parse(rs.getString("birth_date")),
+                        rs.getDouble("sold_value"),
+                        rs.getString("consultant_in_charge")
+                ));
+            }
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+
+        return Optional.empty();
+    }
+
+    @Override
+    public List<EmployeeDTO> findAll() {
+        String sql = "SELECT * FROM employee";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            final ResultSet rs = statement.executeQuery();
+            List<EmployeeDTO> employees = new ArrayList<>();
+
+            while (rs.next()) {
+                employees.add(new EmployeeDTO(
+                        rs.getString("id"),
+                        rs.getString("name"),
+                        LocalDate.parse(rs.getString("birth_date")),
+                        rs.getDouble("sold_value"),
+                        rs.getString("consultant_in_charge")
+                    )
+                );
+            }
+
+            return employees;
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+
+        return List.of();
+    }
+
+    @Override
+    public void delete(String id) {
+        String sql = "DELETE FROM employee WHERE id = ?";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)){
+            statement.setString(1, id);
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+}
