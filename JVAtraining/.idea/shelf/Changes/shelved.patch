+package persistence;
+
+import controller.DTOMapper;
+import persistence.dao.ProductDAO;
+import persistence.dao.SaleDAO;
+import persistence.dto.PersistedSaleDTO;
+import persistence.dto.RegisterSaleDTO;
+
+import java.sql.Connection;
+import java.sql.PreparedStatement;
+import java.sql.ResultSet;
+import java.sql.SQLException;
+import java.util.Objects;
+import java.util.Optional;
+
+public class SQLiteSaleDAO implements SaleDAO {
+    @Override
+    public void register(RegisterSaleDTO dto) {
+        String sqlSale = "INSERT INTO sale (seller, quantity, product) VALUES (?, ?, ?)";
+        String sqlProduct = "UPDATE product SET quantity = ? WHERE code = ?";
+
+        try (Connection connection = ConnectionFactory.getConnection()) {
+            connection.setAutoCommit(false);
+
+            try (PreparedStatement productStatement = connection.prepareStatement(sqlProduct);
+                 PreparedStatement saleStatement = connection.prepareStatement(sqlSale)) {
+
+                int currentProductQuantity = dto.product().getQuantity();
+                int saleQuantity = dto.quantity();
+                int newQuantity = currentProductQuantity - saleQuantity;
+
+                if (newQuantity < 0) {
+                    throw new SQLException("Quantidade insuficiente em estoque para o produto: " + dto.product().getName());
+                }
+
+                System.out.println("DEBUG: Atualizando produto " + dto.product().getSku() + " de " + currentProductQuantity + " para " + newQuantity);
+                productStatement.setInt(1, newQuantity);
+                productStatement.setString(2, dto.product().getSku());
+                int rowsAffectedProduct = productStatement.executeUpdate();
+                System.out.println("DEBUG: Linhas afetadas na atualização do produto: " + rowsAffectedProduct);
+
+                if (rowsAffectedProduct == 0) {
+                    throw new SQLException("Produto com SKU " + dto.product().getSku() + " não encontrado para atualização de estoque.");
+                }
+
+
+                System.out.println("DEBUG: Registrando venda para vendedor: " + dto.seller() + ", quantidade: " + dto.quantity() + ", produto: " + dto.product().getSku());
+                saleStatement.setString(1, dto.seller());
+                saleStatement.setInt(2, dto.quantity());
+                saleStatement.setString(3, dto.product().getSku());
+                int rowsAffectedSale = saleStatement.executeUpdate();
+                System.out.println("DEBUG: Linhas afetadas no registro da venda: " + rowsAffectedSale);
+
+                connection.commit();
+                System.out.println("Venda registrada com sucesso para " + dto.seller());
+
+            } catch (SQLException e) {
+                System.err.println("ERRO na transação de registro/atualização. Realizando rollback.");
+                try {
+                    connection.rollback();
+                } catch (SQLException rollbackEx) {
+                    System.err.println("ERRO ao tentar rollback: " + rollbackEx.getMessage());
+                    rollbackEx.printStackTrace();
+                }
+                throw new RuntimeException("Falha ao registrar venda: " + e.getMessage(), e);
+            }
+
+        } catch (SQLException e) {
+            // Este catch pega erros na obtenção da conexão ou erros de SQL não capturados na transação
+            System.err.println("ERRO fatal de conexão ou SQL ao registrar venda: " + e.getMessage());
+            e.printStackTrace();
+            throw new RuntimeException("Erro ao se conectar ao banco de dados ou erro SQL inesperado.", e);
+        }
+    }
+
+    @Override
+    public Optional<PersistedSaleDTO> findOne(Long id) {
+        String sql = "SELECT * FROM sale WHERE id = ?;";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)) {
+            SQLiteProductDAO dao = new SQLiteProductDAO();
+            statement.setLong(1, id);
+            final ResultSet rs = statement.executeQuery();
+
+            if (rs.next()) {
+                return Optional.of(new PersistedSaleDTO(
+                        rs.getLong("id"),
+                        rs.getString("seller"),
+                        rs.getInt("quantity"),
+                        DTOMapper.fromDto(dao.findOne(rs.getString("product"))
+                                .get()
+                        )));
+            }
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+
+        return Optional.empty();
+    }
+
+    @Override
+    public void update(PersistedSaleDTO dto) {
+        String sqlSale = "UPDATE sale SET seller = ?, quantity = ?, product = ? WHERE id = ?";
+        String sqlProduct = "UPDATE product SET quantity = ? WHERE code = ?";
+
+        try (var connection = ConnectionFactory.getConnection()) {
+            connection.setAutoCommit(false);
+
+            try (var saleStatement = connection.prepareStatement(sqlSale);
+                 var productStatement = connection.prepareStatement(sqlProduct)) {
+
+                int newQuantity = dto.product().getQuantity() - dto.quantity();
+
+                productStatement.setInt(1, newQuantity);
+                productStatement.setString(2, dto.product().getSku());
+                productStatement.executeUpdate();
+
+                saleStatement.setString(1, dto.seller());
+                saleStatement.setInt(2, dto.quantity());
+                saleStatement.setString(3, dto.product().getSku());
+                saleStatement.setLong(4, dto.id());
+                saleStatement.executeUpdate();
+
+                connection.commit();
+            } catch (SQLException e) {
+                connection.rollback();
+                throw e;
+            }
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+
+    @Override
+    public void delete(Long id) {
+        String sql = "DELETE FROM sale WHERE id = ?";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql);){
+            statement.setLong(1, id);
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+}
