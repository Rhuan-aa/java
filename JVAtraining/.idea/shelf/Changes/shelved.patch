@@ -0,0 +1,68 @@
+package service;
+
+import costs.PeriodCostDao;
+import costs.PeriodCostDto;
+import persistence.PeriodCostDaoImpl;
+
+import java.util.Comparator;
+import java.util.List;
+import java.util.Optional;
+
+public class FeeCalculator {
+
+    private final PeriodCostDao periodCostDao;
+    private final List<PeriodCostDto> sortedPeriodCosts;
+
+    public FeeCalculator(PeriodCostDao periodCostDao) {
+        this.periodCostDao = periodCostDao;
+        this.sortedPeriodCosts = periodCostDao.findAll().stream()
+                .sorted(Comparator.comparing(PeriodCostDto::hours, Comparator.reverseOrder()))
+                .toList();
+    }
+
+    private record FeeCalculationState(double accumulatedFee, int remainingHours) {
+            public FeeCalculationState applyPeriod(PeriodCostDto period) {
+                int periodHours = period.hours();
+                double periodFee = period.fee();
+
+                int numApplications = 0;
+                if (periodHours > 0) {
+                    numApplications = this.remainingHours / periodHours;
+                }
+
+                double feeFromThisPeriod = numApplications * periodFee;
+                int newRemainingHours = this.remainingHours - (numApplications * periodHours);
+
+                return new FeeCalculationState(this.accumulatedFee + feeFromThisPeriod, newRemainingHours);
+            }
+        }
+
+
+    public double calculateFee(double durationInHours) {
+        int roundedUpHours = (int) Math.ceil(durationInHours);
+
+        if (roundedUpHours <= 0) {
+            return 0.0;
+        }
+
+        Optional<Double> cheapestCoveringBlockCost = sortedPeriodCosts.stream()
+                .filter(period -> period.hours() >= roundedUpHours)
+                .map(PeriodCostDto::fee)
+                .min(Comparator.naturalOrder());
+
+        FeeCalculationState finalGreedyState = sortedPeriodCosts.stream()
+                .reduce(new FeeCalculationState(0.0, roundedUpHours),
+                        FeeCalculationState::applyPeriod,
+                        (state1, state2) -> new FeeCalculationState(state1.accumulatedFee + state2.accumulatedFee,
+                                Math.min(state1.remainingHours, state2.remainingHours))
+                );
+
+        double minFee = finalGreedyState.accumulatedFee;
+
+        if (cheapestCoveringBlockCost.isPresent()) {
+            minFee = Math.min(minFee, cheapestCoveringBlockCost.get());
+        }
+
+        return minFee;
+    }
+}
