+package service;
+
+import model.InstitutionalMember;
+import model.Professor;
+import persistence.InstitutionalMemberRepository;
+
+public class MemberReportService {
+    InstitutionalMemberRepository repo;
+
+    public MemberReportService(InstitutionalMemberRepository repo) {
+        this.repo = repo;
+    }
+
+    public String reportOf(String cpf) {
+        return repo.findById(cpf)
+                .map(this::generateReport)
+                .orElse("Member isn't registered");
+    }
+
+    private String generateReport(InstitutionalMember member) {
+        StringBuilder builder = new StringBuilder();
+        recursiveReport(member, 0, builder);
+        return builder.toString();
+    }
+
+    private void recursiveReport(InstitutionalMember member, int level, StringBuilder builder) {
+        builder.append("    ".repeat(level))
+                .append(String.format("%s | Pontuação: %.1f%n", member, member.calculateScore()));
+
+        if (member instanceof Professor professor) {
+            professor.getMentees().values()
+                    .forEach(m -> recursiveReport(m, level + 1, builder));
+        }
+    }
+}
