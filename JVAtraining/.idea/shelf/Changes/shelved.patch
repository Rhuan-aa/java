+import controller.DtoMapper;
+import model.Player;
+import model.Team;
+import persistence.SQLitePlayerDao;
+import persistence.SQLiteTeamDao;
+import persistence.dao.PlayerDao;
+import persistence.dao.TeamDao;
+import persistence.player.PersistedPlayerDto;
+import persistence.team.PersistedTeamDto;
+
+import java.util.Optional;
+
+public class Main {
+    public static void main(String[] args) {
+        TeamDao teamDao = new SQLiteTeamDao();
+        PlayerDao playerDao = new SQLitePlayerDao();
+
+        // 1. Registrar um time
+        Team barcelona = new Team("Barcelona", "Barcelona", "Zgman", null);
+        Player messi = new Player("Messi", 10, "Atacante", true, barcelona);
+        barcelona.setCaptain(messi);
+
+        // Registrar jogador para obter ID
+        playerDao.register(DtoMapper.toDto(messi));
+        // Buscar jogador para pegar o ID gerado
+        Optional<PersistedPlayerDto> messiPersisted = playerDao.findAll().stream()
+                .filter(p -> p.name().equals("Messi"))
+                .findFirst();
+
+        if (messiPersisted.isEmpty()) {
+            System.out.println("Erro ao registrar jogador.");
+            return;
+        }
+
+        // Atualizar capitão com ID correto
+        barcelona.setCaptain(DtoMapper.fromDto(messiPersisted.get()));
+        // Registrar time
+        teamDao.register(DtoMapper.toDto(barcelona));
+
+        // Buscar time registrado
+        Optional<PersistedTeamDto> teamPersisted = teamDao.findAll().stream()
+                .filter(t -> t.name().equals("Barcelona"))
+                .findFirst();
+
+        if (teamPersisted.isPresent()) {
+            System.out.println("Time registrado: " + teamPersisted.get().name());
+        } else {
+            System.out.println("Erro ao registrar time.");
+        }
+
