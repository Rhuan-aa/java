+package service;
+
+import model.InstitutionalMember;
+import model.Professor;
+import model.Researcher;
+import persistence.InstitutionalMemberRepository;
+import persistence.Repository;
+
+import java.lang.reflect.Member;
+import java.time.LocalDate;
+
+public class MemberRegistrationService {
+    private final InstitutionalMemberRepository repo;
+
+    public MemberRegistrationService(InstitutionalMemberRepository repo) {
+        this.repo = repo;
+    }
+
+
+    public void register(String cpf, String name, LocalDate entryDate, String advisorCpf) {
+        InstitutionalMember maybeAdvisor = advisorCpf == null
+                ? null
+                : repo.findById(advisorCpf).orElse(null);
+
+        if (maybeAdvisor == null) {
+            repo.save(new Researcher(cpf, name, entryDate, null));
+            return;
+        }
+
+        // Promove se necessário
+        if (maybeAdvisor instanceof Researcher researcher) {
+            Professor professor = researcher.graduate();
+            repo.update(professor);
+        }
+
+        // Sempre busca a versão atualizada do orientador
+        Professor advisor = (Professor) repo.findById(advisorCpf).orElseThrow();
+
+        final Researcher newMember = new Researcher(cpf, name, entryDate, advisor);
+        advisor.addGuided(newMember);
+
+        repo.update(advisor);
+        repo.save(newMember);
+    }
+}
