+package br.ifsp.infection.main;
+
+import br.ifsp.infection.model.Bulletin;
+import br.ifsp.infection.model.State;
+import br.ifsp.infection.persistence.SQLiteBulletinDAO;
+import br.ifsp.infection.persistence.dao.BulletinDAO;
+import br.ifsp.infection.persistence.dto.PersistedBulletinDTO;
+import br.ifsp.infection.services.*;
+
+import java.time.LocalDate;
+import java.util.List;
+
+public class Main {
+    public static void main(String[] args) {
+        BulletinDAO dao = new SQLiteBulletinDAO();
+        final var registerService = new RegisterBulletinService(dao);
+        final var updateService = new UpdateBulletinService(dao);
+        final var deleteService = new RemoveBulletinService(dao);
+        final var filterService = new FilterBulletinService();
+        final var statisticsService = new StatisticsService();
+
+        registerService.register("Itapipoca", State.CE, 100, 10, 0.1, LocalDate.now());
+        updateService.update(1, "Araratuama", State.PA, 10, 8, 0.3, LocalDate.now());
+        deleteService.remove(3);
+
+        final List<Bulletin> all = dao.findAll().stream()
+                .map(Main::toEntity)
+                .toList();
+
+
+        filterService.filter(all, null, State.SP, LocalDate.of(2022, 3, 31), LocalDate.of(2022, 5, 31)).forEach(System.out::println);
+        System.out.println(statisticsService.createStatistic(all));
+    }
+
+    private static Bulletin toEntity(PersistedBulletinDTO dto) {
+        return new Bulletin(
+                dto.id(),
+                dto.city(),
+                State.fromName(dto.state()),
+                dto.infected(),
+                dto.deaths(),
+                dto.icuRatio(),
+                LocalDate.parse(dto.date())
+        );
+    }
+}
