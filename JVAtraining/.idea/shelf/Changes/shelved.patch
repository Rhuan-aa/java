+package br.ifsp.infection.persistence;
+
+import br.ifsp.infection.model.Bulletin;
+import br.ifsp.infection.persistence.dao.BulletinDAO;
+import br.ifsp.infection.persistence.dto.PersistedBulletinDTO;
+import br.ifsp.infection.persistence.dto.RegisterBulletinDTO;
+
+import java.sql.ResultSet;
+import java.sql.SQLException;
+import java.util.ArrayList;
+import java.util.List;
+
+public class SQLiteBulletinDAO implements BulletinDAO {
+    @Override
+    public void delete(Integer id) {
+        String sql = """
+                DELETE FROM bulletins WHERE id = ?
+                """;
+
+        try (var statement = ConnectionFactory.createPreparedStatement(sql)) {
+            statement.setInt(1, id);
+        } catch (SQLException e) {
+            throw new RuntimeException();
+        }
+    }
+
+    @Override
+    public boolean existsById(Integer id) {
+        String sql = "SELECT 1 FROM bulletins WHERE id = ?";
+
+        try (var statement = ConnectionFactory.createPreparedStatement(sql)) {
+            statement.setInt(1, id);
+            final ResultSet rs = statement.executeQuery();
+            return rs.next();
+        } catch (SQLException e) {
+            throw new RuntimeException(e);
+        }
+    }
+
+    @Override
+    public List<PersistedBulletinDTO> findAll() {
+        List<PersistedBulletinDTO> bulletins = new ArrayList<>();
+        String sql = "SELECT * FROM bulletins";
+
+        try (var statement = ConnectionFactory.createPreparedStatement(sql)) {
+            final ResultSet rs = statement.executeQuery();
+            while (rs.next()) {
+                bulletins.add(new PersistedBulletinDTO(
+                        rs.getInt("id"),
+                        rs.getString("city"),
+                        rs.getString("state"),
+                        rs.getInt("infected"),
+                        rs.getInt("deaths"),
+                        rs.getDouble("icu_ratio"),
+                        rs.getString("date")
+                ));
+            }
+        } catch (SQLException e) {
+            throw new RuntimeException(e);
+        }
+
+        return bulletins;
+    }
+
+    @Override
+    public void register(RegisterBulletinDTO dto) {
+        String sql = "INSERT INTO bulletins (city, state, infected, deaths, icu_ratio, date) " +
+                "VALUES (?, ?, ?, ?, ?, ?)";
+
+        try (var statement = ConnectionFactory.createPreparedStatement(sql)) {
+            statement.setString(1, dto.city());
+            statement.setString(2, dto.state());
+            statement.setInt(3, dto.infected());
+            statement.setInt(4, dto.death());
+            statement.setDouble(5, dto.icuRatio());
+            statement.setString(6, dto.date());
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            throw new RuntimeException(e);
+        }
+    }
+
+    @Override
+    public void update(PersistedBulletinDTO dto) {
+        String sql = """
+                UPDATE bulletins SET city = ?, state = ?, infected = ?, deaths = ?, icu_ratio = ?, date = ?
+                WHERE id = ?
+                """;
+
+        try (var statement = ConnectionFactory.createPreparedStatement(sql)) {
+            statement.setString(1, dto.city());
+            statement.setString(2, dto.state());
+            statement.setInt(3, dto.infected());
+            statement.setInt(4, dto.deaths());
+            statement.setDouble(5, dto.icuRatio());
+            statement.setString(6, dto.date());
+            statement.setInt(7, dto.id());
+        } catch (SQLException e) {
+            throw new RuntimeException(e);
+        }
+    }
+}
