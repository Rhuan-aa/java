+import java.util.List;
+import java.util.NoSuchElementException;
+import java.util.Objects;
+import java.util.Optional;
+
+public class CommissionService {
+    private final EmployeeDAO dao;
+
+    public CommissionService(EmployeeDAO dao) {
+        this.dao = dao;
+    }
+
+    public double calculateCommission(String id) {
+        final Optional<EmployeeDTO> maybeRoot = dao.findById(id);
+
+        if (maybeRoot.isEmpty())
+            throw new NoSuchElementException("Empregado não encontrado");
+
+        Consultant consultant = (Consultant) fromDto(maybeRoot.get(), null);
+
+        List<Employee> resellers = dao.findAll().stream()
+                .filter(e -> Objects.equals(e.inChargeId(), id))
+                .map(e -> fromDto(e, consultant))
+                .toList();
+
+        resellers.forEach(consultant::addSubordinate);
+
+        return consultant.getCommission();
+    }
+
+    private Employee fromDto(EmployeeDTO dto, Consultant consultant) {
+        return new Reseller(
+                dto.id(),
+                dto.name(),
+                dto.birthDate(),
+                dto.soldValue(),
+                consultant
