+import model.Product;
+import model.Sale;
+import persistence.SQLiteProductDAO;
+import persistence.SQLiteSaleDAO;
+import persistence.dao.ProductDAO;
+import persistence.dao.SaleDAO;
+import persistence.dto.PersistedSaleDTO;
+import persistence.dto.ProductDTO;
+import persistence.dto.RegisterSaleDTO;
+
+import java.math.BigDecimal;
+import java.util.Optional;
+
+public class Main {
+    public static void main(String[] args) {
+        ProductDAO productDAO = new SQLiteProductDAO();
+        SaleDAO saleDAO = new SQLiteSaleDAO();
+
+        // 1. Registrar um produto
+        Product product = new Product("SCL0001", "Computer", 10, BigDecimal.valueOf(4532.99).doubleValue());
+        productDAO.register(product.toDto());
+
+        // 2. Buscar produto
+        Optional<ProductDTO> foundProduct = productDAO.findOne("SCL0001");
+        foundProduct.ifPresent(p -> System.out.println("Produto encontrado: " + p));
+
+        // 3. Atualizar produto
+        Product updatedProduct = new Product("SCL0001", "Computer Gamer", 8, 5000.00);
+        productDAO.update(updatedProduct.toDto());
+
+        // 4. Registrar uma venda
+        RegisterSaleDTO saleDTO = new RegisterSaleDTO("Rhuan A. A. Boni", 2, updatedProduct);
+        saleDAO.register(saleDTO);
+
+        // 5. Buscar venda (exemplo com id 1)
+        Optional<PersistedSaleDTO> foundSale = saleDAO.findOne(5L);
+        foundSale.ifPresent(s -> System.out.println("Venda encontrada: " + s));
+
+        if (foundSale.isEmpty()) System.out.println("Venda não encontrada");
+
+        // 6. Deletar venda (exemplo com id 1)
+        saleDAO.delete(1L);
+
+        // 7. Deletar produto (deleta também as vendas relacionadas)
+        productDAO.delete("SCL0001");
+
+    }
+}
