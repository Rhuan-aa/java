+public class EntryTicketDaoImpl implements TicketDao {
+
+    @Override
+    public void save(EntryTicketDto dto) {
+        String sql = "INSERT INTO ticket (id, plate, entry) VALUES (?, ?, ?)";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)){
+            statement.setString(1, dto.id());
+            statement.setString(2, dto.plate());
+            statement.setString(3, dto.entry());
+
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+
+    @Override
+    public void updateExit(ExitTicketDto dto) {
+        String sql = "UPDATE exit = ?, fee = ? WHERE plate = ?";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)){
+            statement.setString(1, dto.exit());
+            statement.setDouble(2, dto.fee());
+            statement.setString(3, dto.plate());
+
+            statement.executeUpdate();
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+    }
+
+    @Override
+    public Optional<EntryTicketDto> findOpenTicked(String plate) {
+        String sql = "SELECT * FROM ticket WHERE plate = ? AND exit IS NULL";
+
+        try (var statement = ConnectionFactory.getPreparedStatement(sql)){
+            statement.setString(1, plate);
+            final ResultSet rs = statement.executeQuery();
+
+            if (rs.next()) {
+                return Optional.of(new EntryTicketDto(
+                        rs.getString("id"),
+                        rs.getString("plate"),
+                        rs.getString("entry")
+                ));
+            }
+
+        } catch (SQLException e) {
+            e.printStackTrace();
+        }
+
+        return Optional.empty();
+    }
+}
