+package model;
+
+import java.time.LocalDate;
+import java.util.HashMap;
+import java.util.Map;
+
+public final class Professor extends InstitutionalMember {
+    private final Map<String, InstitutionalMember> mentees;
+
+    public Professor(String cpf, String name, LocalDate entryDate, Professor advisor) {
+        super(cpf, name, entryDate, advisor);
+        this.mentees = new HashMap<>();
+    }
+
+    @Override
+    public double calculateScore() {
+        return publications.stream()
+                .mapToDouble(p -> 10 * p.getImpactFactor())
+                .sum() + mentees.values().stream()
+                .mapToDouble(m -> m.calculateScore() / 2)
+                .sum();
+    }
+
+    public void addGuided(InstitutionalMember guided){
+        mentees.putIfAbsent(guided.cpf, guided);
+    }
+
+    public Map<String, InstitutionalMember> getMentees() {
+        return new HashMap<>(mentees);
+    }
+}
