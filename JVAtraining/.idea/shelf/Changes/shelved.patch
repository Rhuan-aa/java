+package br.ifsp.services;
+
+import br.ifsp.persistence.EmployeeDAO;
+
+import java.util.List;
+import java.util.NoSuchElementException;
+import java.util.Objects;
+import java.util.Optional;
+
+public class RemoveEmployeeService {
+    private final EmployeeDAO dao;
+
+    public RemoveEmployeeService(EmployeeDAO dao) {
+        this.dao = dao;
+    }
+
+    public void remove(String id) {
+        Optional<EmployeeDTO> maybeDto = dao.findById(id);
+        if (maybeDto.isEmpty())
+            throw new NoSuchElementException("Empregado não registrado");
+
+        EmployeeDTO employee = maybeDto.get();
+
+        List<EmployeeDTO> employees = dao.findAll().stream()
+                .filter(p -> Objects.equals(p.inChargeId(), id))
+                .toList();
+
+        if (!employees.isEmpty()) {
+            for (EmployeeDTO dto : employees) {
+                EmployeeDTO newDto = new EmployeeDTO(
+                        dto.id(),
+                        dto.name(),
+                        dto.birthDate(),
+                        dto.soldValue(),
+                        employee.inChargeId()
+                );
+
+                dao.update(newDto);
+            }
+        }
+
+        dao.delete(id);
+    }
+}
