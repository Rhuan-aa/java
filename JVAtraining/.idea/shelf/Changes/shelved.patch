+package model;
+
+import model.exception.PlayerAlreadyHiredException;
+
+import java.util.ArrayList;
+import java.util.List;
+import java.util.NoSuchElementException;
+import java.util.Objects;
+
+public class Team {
+    private int id;
+    private String name;
+    private String baseLocation;
+    private String coachName;
+    private Player captain;
+    private final List<Player> players = new ArrayList<>();
+
+    public Team(String name, String baseLocation, String coachName, Player captain) {
+        this.name = name;
+        this.baseLocation = baseLocation;
+        this.coachName = coachName;
+        this.captain = captain;
+    }
+
+    public Team(int id, String name, String baseLocation, String coachName, Player captain) {
+        this.id = id;
+        this.name = name;
+        this.baseLocation = baseLocation;
+        this.coachName = coachName;
+        this.captain = captain;
+    }
+
+    @Override
+    public boolean equals(Object o) {
+        if (o == null || getClass() != o.getClass()) return false;
+        Team team = (Team) o;
+        return Objects.equals(name, team.name) && Objects.equals(baseLocation, team.baseLocation);
+    }
+
+    @Override
+    public int hashCode() {
+        return Objects.hash(name, baseLocation);
+    }
+
+    public void addPlayer(Player player) {
+        if (players.contains(player))
+            throw new PlayerAlreadyHiredException("O jogador " + player.getName() + " já está contratado");
+        player.setTeam(this);
+        players.add(player);
+    }
+
+    public void removePlayer(Player player) {
+        if (!players.contains(player))
+            throw new NoSuchElementException("O jogador " + player.getName() + " não está contratado");
+        players.remove(player);
+    }
+
+    public void substitute(Player substitute, Player starter) {
+        if (substitute.isFielded() != starter.isFielded()){
+            starter.swapField();
+            substitute.swapField();
+            if (starter == captain) setCaptain(substitute);
+            return;
+        }
+
+        throw new IllegalArgumentException("Ambos estão na mesma condição");
+    }
+
+    public List<Player> getFieldedPlayers() {
+        return players.stream()
+                .filter(Player::isFielded)
+                .toList();
+    }
+
+    public List<Player> getOutfieldedPlayers() {
+        return players.stream()
+                .filter(player -> !player.isFielded())
+                .toList();
+    }
+
+    public int getId() {
+        return id;
+    }
+
+    public String getName() {
+        return name;
+    }
+
+    public void setName(String name) {
+        this.name = name;
+    }
+
+    public String getBaseLocation() {
+        return baseLocation;
+    }
+
+    public void setBaseLocation(String baseLocation) {
+        this.baseLocation = baseLocation;
+    }
+
+    public String getCoachName() {
+        return coachName;
+    }
+
+    public void setCoachName(String coachName) {
+        this.coachName = coachName;
+    }
+
+    public Player getCaptain() {
+        return captain;
+    }
